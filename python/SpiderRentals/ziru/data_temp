<script type="text/javascript">
//var ZiRoomCompareUrl = '/?_p=compare' ;
var ZiRoomCompareUrl = '/z/vc/' ;
initZiRoomCompare() ;

function url_date11(){
var pricedown = $("#pricedown").val();
var priceup = $("#priceup").val();
var put_u = $("#put_u").val();
//alert(put_u);
if (pricedown=="不限最低" || pricedown==""){ pricedown=0;}
if (priceup=="不限最高" || priceup==""){ priceup=0;}
if (pricedown==0 && priceup==0){ alert("请您输入价格区间！");return ;}
if (isNaN($("#pricedown").val())) {
 $("#pricedown").val("");
 return ;
}
if (isNaN($("#priceup").val())) {
$("#priceup").val("");
return ;
}
if (pricedown*1>=priceup*1){ alert("最低价格不能大于等于最高价格！");return ;}
    if(put_u=='zr'){
        window.location.href = "/z/nl/z2-r"+pricedown+"TO"+priceup+".html";
    }else if(put_u=='zz'){
        window.location.href = "/z/nl/z1-r"+pricedown+"TO"+priceup+".html";
    }else{
        window.location.href = "/z/nl/r"+pricedown+"TO"+priceup+".html";
    }
}
</script>
<!-- 对比结束-->

<!--搜做框智能感应 -->
<script type="text/javascript">
$(function(){
 var step = -1;
 var oldValue="";
 var listTimer=null;
 $(document).click(function(){
 $(".t_ajaxbox").hide();
     });
 $("#i_q_keyword_1").keyup(function(ev){
var oEvent=ev||event;
        var keyCode = oEvent.keyCode;
        var oTxt = $.trim($("#i_q_keyword_1").val());

        if(oTxt==''){
            return;
        }

        if(oEvent.keyCode==38 || oEvent.keyCode==40)
        {
            return;
        }

        if(oEvent.keyCode==13)
        {

            $('#t_pagelistSearchBtn').click();
        }
        clearInterval(listTimer);

        listTimer = setTimeout(function(){
            $.ajax({
 type: "POST",
 dataType:'json',
 url: "/?_p=nlist&_a=suggest",
 data: {wd:oTxt},
 success: function(msg){
if(msg.length==0)
{
  $(".t_ajaxbox").hide();
  return false;
}
var ulstr = '';

$(".t_ajaxbox").find('ul').html(ulstr);
for(var i=0;i<msg.length;i++)
{
    
    if(!msg[i].name){
         $(".t_ajaxbox").hide();
        return;
    }
  var goUrl = '';
  var txt   = '';
  if(msg[i].flag==1)
  {
  //goUrl = "/z/nl/sub/o5-s"+msg[i].sublinecode+"-t"+msg[i].subwaycode+".html";
  goUrl = "/z/nl/sub/";
  txt = msg[i].sublinename;
  }
  else if(msg[i].flag==2)
  {
  //goUrl = "/z/nl/d"+msg[i].districtcode+"-b"+msg[i].bizcirclecode+".html";
  goUrl = "/z/nl/";
  txt = msg[i].districtname;
      }
  else
  {
  goUrl = '';
  txt = '';
  }
  ulstr+='<li data="'+goUrl+'"><span class="keyword" style="margin-right:5px;">'+msg[i].name+'</span>';
  ulstr+= txt;
  <!--ulstr+='<span class="numword">约'+msg[i].housenum+'条房源</span></li>';-->
}
$(".t_ajaxbox").find('ul').html(ulstr).end().show();
}
});
        },500);



        oldValue=oTxt;
        step=-1;
     });

 $("#i_q_keyword_1").keydown(function(ev){
var oEvent=ev||event;
        var keyCode = oEvent.keyCode;

        if(oEvent.keyCode==40)  //↓
        {
            step++;

            if(step==$(".t_ajaxbox").find('ul').find('li').length)
            {
                step=-1;
            }
            $(".t_ajaxbox ul li").removeClass("keyhover");

            if(step!=-1)
            {
$(".t_ajaxbox").find('ul').find('li').eq(step).addClass("keyhover");
$("#i_q_keyword_1").attr('_url',$(".t_ajaxbox").find('ul').find('li').eq(step).attr('data'));
                $("#i_q_keyword_1").val($(".t_ajaxbox").find('ul').find('li').eq(step).find(".keyword").text());
            }
            else
            {
                $("#i_q_keyword_1").val(oldValue);
$("#i_q_keyword_1").attr('_url','');
            }
        }
        else if(oEvent.keyCode==38)
        {
            step--;

            if(step==-2)
            {
                step=$(".t_ajaxbox").find('ul').find('li').length-1;
            }

            $(".t_ajaxbox ul li").removeClass("keyhover");

            if(step!=-1)
            {
                $(".t_ajaxbox").find('ul').find('li').eq(step).addClass("keyhover");
$("#i_q_keyword_1").attr('_url',$(".t_ajaxbox").find('ul').find('li').eq(step).attr('data'));
                $("#i_q_keyword_1").val($(".t_ajaxbox").find('ul').find('li').eq(step).find(".keyword").text());
            }
            else
            {
                $("#i_q_keyword_1").val(oldValue);
$("#i_q_keyword_1").attr('_url','');
            }
        }
        if(oEvent.keyCode==38 || oEvent.keyCode==40)
        {
            return false;
        }
 });

 $(".t_ajaxbox ul").find("li").live("click",function(){
   var s_url = '';
   if($(this).attr('data')!='')
s_url = ""+$(this).attr('data')+'?qwd='+encodeURIComponent($(this).find(".keyword").text());
   else
s_url = "/z/nl/?qwd="+encodeURIComponent($(this).find(".keyword").text());
   window.location = s_url;
 });
 $(".t_ajaxbox ul").find("li").live("mouseover mouseout",function(event){
  $(".t_ajaxbox ul li").removeClass("keyhover");
  if (event.type == 'mouseover') {
$(this).addClass("keyhover");
$("#i_q_keyword_1").attr('_url',$(this).attr('data'));
step = $(this).index();
  } else {
$(this).removeClass("keyhover");
$("#i_q_keyword_1").attr('_url','');
step = $(this).index();
  }
 });

 $("#JumpToPage").keydown(function(ev){
        var oEvent=ev||event;
        var keyCode = oEvent.keyCode;
var pg = $(this).val();
        if(keyCode==13)
{
        window.location = "/z/nl/z3.html?p="+pg;
}
     });
 $(".go").click(function(ev){
var pg = $("#JumpToPage").val();
        window.location = "/z/nl/z3.html?p="+pg;
     });
});
    $(".btn_sub").click(function(){

var $this=$(this);
var iq = $("#i_q_keyword_"+$this.attr("name")).val();
if (iq=="请输入地名，地铁线路，站点名..." || iq=="")return ;
var _url = $("#i_q_keyword_"+$this.attr("name")).attr('_url');
   if(typeof(_url)!='undefined' && _url!='')
     var zr = ""+$("#i_q_keyword_"+$this.attr("name")).attr('_url');
   else
     var zr = ""+"/z/nl/z3.html";
var query=zr+"?qwd="+$("#i_q_keyword_"+$this.attr("name")).val();
window.location.href = encodeURI(query) ;
});
$("#i_q_keyword_1").keydown(function(e){
if(e.keyCode == 13)
{
var iq = $("#i_q_keyword_1").val();
if (iq=="请输入地名，地铁线路，站点名..." || iq=="")return ;
var zr = ""+"/z/nl/z3.html";
var query=zr+"?qwd="+$("#i_q_keyword_1").val();
window.location.href = encodeURI(query) ;
}
});
</script>

